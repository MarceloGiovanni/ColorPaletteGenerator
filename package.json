    "@mui/material": "^6.1.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "quicktype": "^23.0.170",
    "typescript": "^5.7.2",
    "vite": "^6.0.3"
  }
}
